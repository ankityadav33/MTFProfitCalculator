{
  "manifest_version": 3,

  "name": "MTF live Profits Calculator",
  "description": "This extension will calculate profits on MTF page of ICICI Direct",
  "version": "1.0",

  "action": {
   "default_popup": "popup.html",
    "default_icon": "icon128.png"
  },
  "icons": {
    "19": "icon19.png",
    "38": "icon38.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "host_permissions": [ "https://secure.icicidirect.com/" ],
  "permissions": [
    "scripting",
    "activeTab"
  ]
}